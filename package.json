{
  "name": "vscode-brack",
  "version": "0.1.0",
  "description": "VSCode extension for brack",
  "main": "dist/extension.js",
  "scripts": {
    "compile": "tsc -p ."
  },
  "author": "Mutsuha Asada",
  "activationEvents": [
    "onLanguage:brack"
  ],
  "contributes": {
    "languages": [
      {
        "id": "brack",
        "extensions": [
          ".[]"
        ]
      }
    ],
    "configurationDefaults": {
      "[brack]": {
        "editor.semanticHighlighting.enabled": true
      }
    }
  },
  "licenses": [
    {
      "type": "MIT"
    },
    {
      "type": "Apache-2.0"
    }
  ],
  "dependencies": {
    "@types/vscode": "^1.85.0",
    "vscode": "^1.1.37",
    "vscode-languageclient": "^9.0.1"
  },
  "engines": {
    "vscode": "^1.52.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
